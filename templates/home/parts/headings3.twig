{% if Text3 is not null %}
    <h4>
        {% if Text3.hyperlinks is empty %}
            {{ Text3.contentTitle|raw }}
        {% else %}
            <a target="_blank" href="{{ Text3.hyperlinks }}">{{ Text3.contentTitle|raw }}</a>
        {% endif %}
    </h4>
    {% if is_granted('ROLE_ADMIN') %}
        <a target="_blank" href="{{ path('cms_copy_edit', {'id': Text3.id}) }}">
            <i style="color: red" class="fa fa-pen"></i>
        </a>
    {% endif %}
{% endif %}

