{% extends 'base.html.twig' %}

{% block title %}File Upload{% endblock %}

{% block body %}


    <h1 style="color: red">File Upload</h1>
    {{ form_start(form) }}
    {{ form_row(form.file) }}
    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}


    <a class="btn btn-outline-primary btn-sm mt-2" href="{{ path('app_home') }}">Home</a>
{% endblock %}
