{{ form_start(form) }}
<h4 style="color: red">Document History</h4>
<div class="row">
    <div class="col-2">
        {{ form_row(form.editedBy) }}
        {{ form_row(form.date) }}
    </div>
    <div class="col-2">
        {{ form_row(form.document) }}
        {{ form_row(form.documentId) }}
    </div>
</div>

<div class="row">
    <div class="col-9">
        <h4 style="color: red">Notes</h4>
        {{ form_row(form.notes) }}
    </div>
</div>


<button class="btn btn-success btn-sm">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}


